#include <stdio.h>
#include <stdlib.h>

/* run this program using the console pauser or add your own getch, system("pause") or input loop */

int main(){
	int i;
	int a=0;
	for(i=1; i<=20; i++){
		if(i%5==0){
			a=a+i;
		}
	}
	printf("1到20質數%d\n",a);
	int x;
	int b=0;
	for(x=21; x<=40; x++){
		if(x%9==0){
			b=b+x;
		}
	}
	printf("21到40質數%d\n",b);
	printf("總和%d",a+b);
	return 0;
}

#include <stdio.h>
#include <stdlib.h>

/* run this program using the console pauser or add your own getch, system("pause") or input loop */

int main() {
	int i;
	for(i=1; i<=26; i+=5){
		printf("%d ", i);
	} 
	return 0;
}

#include <stdio.h>
#include <stdlib.h>

/* run this program using the console pauser or add your own getch, system("pause") or input loop */

int main() {
	int i;
 
	for(i=50; i>=1; i--){
		if(i%2==0 || i%3==0){
			printf("00 ");
		
		}
		else{
			printf("%d ",i);
		}
		if((i-1)%5==0){
			printf("\n");
		}
	} 

	return 0;
}

